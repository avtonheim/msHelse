<Page loaded="onLoad">
  <Page.actionBar>
       <ActionBar class="action-bar">
         <StackLayout>
            <Image src="~/images/tittel.png" class="iconTitle" ios.position="center"></Image>
         </StackLayout>
       </ActionBar>
   </Page.actionBar>
   <ScrollView>
   <StackLayout class="p-20">
     <Label class="h1" text="Oversikt over tekst"></Label>
     <Label class="body" textWrap="true" text="Dette er ein oversikt over tekst."></Label>
       <ListView items="{{ Texts }}" itemTap="textView">
         <ListView.itemTemplate>
             <Label class="textDay" text="{{ date }}"/>
         </ListView.itemTemplate>
       </ListView>
     </StackLayout>
</ScrollView>
</Page>
