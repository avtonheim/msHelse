<Page loaded="onLoad">
  <Page.actionBar>
       <ActionBar class="action-bar">
         <StackLayout>
            <Image src="~/images/tittel.png" class="iconTitle" ios.position="center"></Image>
         </StackLayout>
       </ActionBar>
   </Page.actionBar>
   <ScrollView>
   <StackLayout class="p-20">
     <Label class="h1" text="Oversikt over tekst"></Label>
     <Label class="body" textWrap="true" text="Dette er ein oversikt over tekst."></Label>
     <StackLayout class="navButton">
       <Label text="I går"></Label>
     </StackLayout>
     <StackLayout class="navButton">
       <Label text="Dei 7 siste dagane"></Label>
     </StackLayout>
     <StackLayout class="navButton">
       <Label text="Den siste månaden"></Label>
     </StackLayout>
     <StackLayout class="navButton" onTap="textView">
       <Label text="Alt"></Label>
     </StackLayout>

     </StackLayout>
</ScrollView>
</Page>
