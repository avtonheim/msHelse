<Page loaded="onLoaded">
  <Page.actionBar>
         <ActionBar title="mHelse" class="action-bar">
           <StackLayout>
              <Image src="~/images/tittel.png" class="iconTitle" ios.position="center"></Image>
           </StackLayout>
           <ActionItem text="Neste" tap="onNavText" ios.position="right"/>
         </ActionBar>
     </Page.actionBar>

    <StackLayout class="diaryComp">
       <Progress value="{{ someProperty }}"/>
      <Label class="titleMain" style="font-size: 35pt;" text="Kva har prega dagen?"/>
      <Label class="body" text="Sveip på grafen for å registrere" textWrap="true"/>
      <Button text="console.log" tap="{{ select }}"></Button>

      <StackLayout class="symptomCont">
        <Label text="Vannlatning" tap="{{ insert }}"/>
        <!--
        <GridLayout columns="*,*" rows="*">
          <Button class="eventBtn" col="0" row="0" text="Morgon"></Button>
          <Button class="eventBtn" col="1" row="0" text="Kveld"></Button>
        </GridLayout>-->
      </StackLayout>
      <StackLayout class="symptomCont">
        <Label text="Søvnproblem" tap="{{ insert }}"/>
      </StackLayout>
      <StackLayout class="symptomCont">
        <Label text="Hukommelse" tap="{{ insert }}"/>
      </StackLayout>
      <StackLayout class="symptomCont">
        <Label text="Fatigue" tap="{{ insert }}"/>
      </StackLayout>
      <StackLayout class="symptomCont">
        <Label text="Klikk for å legge til eit anna symptom"/>
      </StackLayout>

    </StackLayout>
</Page>
