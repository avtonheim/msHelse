<Page navigatingTo="onNavigatingTo">
  <Page.actionBar>
         <ActionBar title="mHelse" class="action-bar">
           <StackLayout>
              <Image src="~/images/tittel.png" class="iconTitle" android.position="actionBar" ios.position="center"></Image>
           </StackLayout>
           <ActionItem text="Neste" tap="onNavText" ios.position="right"/>
         </ActionBar>
     </Page.actionBar>
  <ScrollView>
    <StackLayout class="diaryComp">
       <Progress value="{{ someProperty }}"/>
      <Label class="titleMain" style="font-size: 35pt;" text="Kva har prega dagen?"/>
      <Label class="body" text="Trykk på dei kvite knappane for å registrere" textWrap="true"/>
      <Button text="console.log" tap="{{ selectEverything }}"></Button>

      <StackLayout class="symptomCont" style="height: 200px;">
        <TextField id="inputSelf" hint="Klikk for å legge til eit anna symptom"/>
        <GridLayout columns="*,*" rows="*">
          <Button class="eventBtn" col="0" row="0" value="1" text="Morgon"></Button>
          <Button class="eventBtn" col="1" row="0" value="1" text="Kveld"></Button>
        </GridLayout>
      </StackLayout>

      <StackLayout class="symptomCont">
        <Label text="Vannlatning" tap="{{ insert }}"/>
        <GridLayout columns="*,*" rows="*">
          <Button class="eventBtn" col="0" row="0" value="1" context="Vannlatning" text="Morgon" tap="{{ insert }}"></Button>
          <Button class="eventBtn" col="1" row="0" value="1" context="Vannlatning" text="Kveld" tap="{{ insert }}"></Button>
        </GridLayout>
      </StackLayout>
      <StackLayout class="symptomCont">
        <Label text="Søvnproblem"/>
        <GridLayout columns="*,*" rows="*">
          <Button class="eventBtn" col="0" row="0" value="1" context="Søvnproblem" text="Morgon" text="Morgon" tap="{{ insert }}"></Button>
          <Button class="eventBtn" col="1" row="0" value="1" context="Søvnproblem" text="Kveld" text="Kveld" tap="{{ insert }}"></Button>
        </GridLayout>
      </StackLayout>
      <StackLayout class="symptomCont">
        <Label text="Hukommelse" tap="{{ insert }}"/>
        <GridLayout columns="*,*" rows="*">
          <Button class="eventBtn" col="0" row="0" value="1" context="Hukommelse" text="Morgon" text="Morgon" tap="{{ insert }}"></Button>
          <Button class="eventBtn" col="1" row="0" value="1" context="Hukommelse" text="Kveld" text="Kveld" tap="{{ insert }}"></Button>
        </GridLayout>
      </StackLayout>
      <StackLayout class="symptomCont">
        <Label text="Fatigue" tap="{{ insert }}"/>
        <GridLayout columns="*,*" rows="*">
          <Button class="eventBtn" col="0" row="0" value="1" context="Fatigue" text="Morgon" tap="{{ insert }}"></Button>
          <Button class="eventBtn" col="1" row="0" value="1" context="Fatigue" text="Kveld" tap="{{ insert }}"></Button>
        </GridLayout>
      </StackLayout>

    </StackLayout>
  </ScrollView>
</Page>
