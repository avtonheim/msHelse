<Page loaded="onLoaded">
  <Page.actionBar>
         <Image src="images/ikon.png" class="action-image"></Image>
         <ActionBar title="mHelse" class="action-bar"></ActionBar>
         <NavigationButton text="Tilbake" tap="tapHome"/>
     </Page.actionBar>

    <StackLayout>
      <Progress value="{{ someProperty }}"/>
      <Label class="content" text="Symptom" style="font-weight: bold;"/>
      <Label class="content" text="Her kan du legge inn dine symptom sidan sist du fullførte dagboka di. Du kan velje fleire symptom" textWrap="true"/>

      <GridLayout rows="*,*,*" columns="*,*" width="350" height="250">
        <Button row="0" col="0" text="Fatugue" tap="onTap"/>
        <Button row="0" col="1" text="Søvnproblem" tap="onTap"/>
        <Button row="1" col="0" text="Husk" tap="onTap"/>
        <Button row="1" col="1" text="Synsproblem" tap="{{ onTap }}"/>

    </GridLayout>
      <Button class="nxtBtn" text="Neste" tap="onNavText"/>
    </StackLayout>
</Page>
