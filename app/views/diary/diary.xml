<Page loaded="onLoaded">
  <Page.actionBar>

         <ActionBar title="mHelse" class="action-bar">
           <StackLayout>
              <Image src="~/images/tittel.png" class="iconTitle" ios.position="center"></Image>
           </StackLayout>
           <ActionItem text="Tilbake" tap="tapHome"/>
         </ActionBar>
     </Page.actionBar>
    <StackLayout id="diary">
      <Progress value="{{ someProperty }}" />
      <StackLayout class="content">
        <Label class="title" text="Velkommen til dagboka di!"/>
        <Label text="I dagboka vil du bli spurt om fire ting:" textWrap="true"/>
      </StackLayout>
        <GridLayout rows="*" columns="*" height="400">
          <StackLayout row="0" col="0">
            <Image src="~/images/standebilete.png" />
          </StackLayout>
        </GridLayout>
        <StackLayout>
          <Button class="startEndBtn" text="Start registrering" tap="onNavPinch"/>
        </StackLayout>
  </StackLayout>



</Page>
