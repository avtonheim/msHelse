<Page loaded="onLoaded">
  <Page.actionBar>
         <Image src="~/images/ikon.png"></Image>
         <ActionBar title="mHelse" class="action-bar"></ActionBar>
         <NavigationButton text="Tilbake" tap="tapHome"/>
     </Page.actionBar>
    <StackLayout>
      <Progress value="{{ someProperty }}" />
      <StackLayout class="content">
        <Label class="title" text="Velkommen til dagboka di!"/>
        <Label text="I dagboka vil du bli spurt om fire ting:" textWrap="true"/>
      </StackLayout>
        <GridLayout rows="*" columns="*" height="400">
          <StackLayout row="0" col="0">
            <Image src="~/images/standebilete.png" />
          </StackLayout>
        </GridLayout>
        <StackLayout>
          <Button class="nxtBtn" text="Neste" tap="onNavPinch"/>
        </StackLayout>
  </StackLayout>



</Page>
