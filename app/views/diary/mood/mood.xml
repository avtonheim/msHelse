<Page loaded="onLoaded">
  <Page.actionBar>
         <ActionBar title="mHelse" class="action-bar">
           <StackLayout>
              <Image src="~/images/tittel.png" class="iconTitle" android.position="actionBar" ios.position="center"></Image>
           </StackLayout>
           <ActionItem text="Neste" tap="navSymptom" ios.position="right"/>
           </ActionBar>
     </Page.actionBar>
     <StackLayout>
       <Progress value="{{ someProperty }}" />
     </StackLayout>
    <StackLayout class="diaryComp">
          <StackLayout>
            <Label class="titleMain" style="font-size: 35pt;" text="Korleis har du det?" textWrap="true"/>
            <Label text="Klikk på valgt knapp for å registrere."/>
          </StackLayout>
          <Button class="btnRegister" style="background-color: #F55D5D; color: white; font-size: 20pt;" value="0" text="Elendig" tap="{{ insert }}">
          </Button>
          <Button class="btnRegister" style="background-color: #F5E35D; color: white; font-size: 20pt;" value="5"  text="Middels" tap="{{ insert }}">
          </Button>
          <Button class="btnRegister" style="background-color: #0DA52A; color: white; font-size: 20pt;" value="10" text="Bra" tap="{{ insert }}">
          </Button>
    </StackLayout>
</Page>
