<Page loaded="onLoad">
  <Page.actionBar>
       <ActionBar class="action-bar">
         <StackLayout>
            <Image src="~/images/tittel.png" class="iconTitle" android.position="actionBar" ios.position="center"></Image>
         </StackLayout>
       </ActionBar>
   </Page.actionBar>
   <ScrollView>
   <StackLayout class="p-20" >
     <Label class="h1" text="Konsultasjon"></Label>
     <Label class="body" textWrap="true" text="Dette er ein større oversikt over data om sjukdomen. Denne funksjonen skal støtte konsultasjonen mellom lege og pasient."></Label>
    <StackLayout class="graphContainer">
       <Label class="h2" text="Symptom"></Label>
       <Label class="textExpl" textWrap="true" text="Sidan sist konsultasjon har symptom XX vore mest framtredande."></Label>
       <Label class="textExpl" textWrap="true" text="Symptomet hadde XX tilfelle om morgonen og XX tilfelle om kvelden."></Label>
       <Label class="textInBox" textWrap="true" text="Dei siste 6 månadane."></Label>
       <StackLayout id="symptomGraph">
      </StackLayout>
    </StackLayout>
    <StackLayout class="graphContainer">
      <Label class="h2" text="Dagsform"></Label>
      <Label class="textExpl" text="Sidan sist konsultasjon har du hatt det xx i gjennomsnitt. Månad XX var din beste månad." textWrap="true"></Label>
      <Label class="textInBox" textWrap="true" text="Dei siste 6 månadane."></Label>
      <StackLayout id="moodGraph">
      </StackLayout>
  </StackLayout>
</StackLayout>
</ScrollView>
</Page>
