<Page loaded="onLoad">
  <Page.actionBar>
       <ActionBar class="action-bar">
         <StackLayout>
            <Image src="~/images/tittel.png" class="iconTitle" android.position="actionBar" ios.position="center"></Image>
         </StackLayout>
       </ActionBar>
   </Page.actionBar>
   <ScrollView>
   <StackLayout class="p-10" >
     <Label class="h1" text="Graf over dagbok"></Label>
     <Label class="body" textWrap="true" text="Dette er ein oversikt over dine symptom og dagsform."></Label>

     <StackLayout class="graphContainer">
       <Label class="h2" text="Symptom"></Label>
       <StackLayout id="symptomTextVisual">
       </StackLayout>

       <SegmentedBar>
         <SegmentedBar.items>
           <SegmentedBarItem title="Oversikt sist mÃ¥nad"/>
           <SegmentedBarItem title="Detaljert info" tap="detailedOverview" />
          </SegmentedBar.items>
        </SegmentedBar>


       <StackLayout id="symptomGraph">
      </StackLayout>
    </StackLayout>

     <StackLayout class="graphContainer">
      <Label class="h2" text="Dagsform"></Label>

      <StackLayout id="moodTextVisual">
      </StackLayout>
      <Label class="textInBox" textWrap="true" text="Dei siste 7 dagane."></Label>
      <StackLayout id="moodGraph">
      </StackLayout>
    </StackLayout>
</StackLayout>
</ScrollView>
</Page>
