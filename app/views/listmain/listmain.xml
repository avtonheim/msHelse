<Page loaded="onLoaded">
  <Page.actionBar>
         <ActionBar title="Gjøremål" class="action-bar">
         </ActionBar>
     </Page.actionBar>
  <StackLayout>
    <StackLayout class="content">
    <Image class="mainComp" src="~/images/gjoremal_ny.png" height="400px" width="400px"/>
    <Label class="titleMain" text="Gjøremål"/>
    </StackLayout>
    <StackLayout class="navButton" style="background-color:#DE8282;" tap="navList">
      <Label text="Planlegg noe kjekt!"/>
    </StackLayout>
    <StackLayout class="navButton" style="background-color:#DE8282;" tap="navFinish">
      <Label text="Se fullførte mål"/>
    </StackLayout>
  </StackLayout>
</Page>
