<Page loaded="onLoaded">
  <Page.actionBar>
         <ActionBar class="action-bar">
           <StackLayout>
              <Image src="~/images/tittel.png" class="iconTitle" ios.position="center"></Image>
           </StackLayout>
         </ActionBar>
     </Page.actionBar>
  <StackLayout>
    <StackLayout class="content">
    <Image class="mainComp" src="~/images/ovelse_ny.png" height="400px" width="400px"/>
    <Label class="titleMain" text="Øvingar"/>
    <Label text="Her vil du finne ulike øvelser for trening"/>
    </StackLayout>
    <StackLayout class="navButton">
      <Label text="Mindfullness" tap="MindfullnessTap"/>
    </StackLayout>
    <StackLayout class="navButton">
      <Label text="Gå tur"/>
    </StackLayout>
  </StackLayout>
</Page>
