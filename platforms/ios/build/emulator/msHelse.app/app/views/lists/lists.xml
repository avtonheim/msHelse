<Page  xmlns:lv="nativescript-ui-listview" navigatingTo="onNavigatingTo">
    <Page.actionBar>
        <ActionBar class="action-bar">
          <StackLayout>
             <Image src="~/images/tittel.png" class="iconTitle" android.position="actionBar" ios.position="center"></Image>
          </StackLayout>
            <ActionBar.actionItems>
                <ActionItem text="Ny" tap="{{ insert }}" ios.position="right" />
            </ActionBar.actionItems>
        </ActionBar>
    </Page.actionBar>
     <lv:RadListView class="listLayout">
        <lv:RadListView.listViewLayout>
            <lv:ListViewLinearLayout scrollDirection="Vertical"/>
        </lv:RadListView.listViewLayout>
        <lv:RadListView.itemTemplate>
            <StackLayout class="list-group-item">
                <Label text="{{ id }}" class="list-group-item-heading" />
                <Label text="{{ list_name }}" textWrap="true" class="list-group-item-text" />
            </StackLayout>
        </lv:RadListView.itemTemplate>
    </lv:RadListView>
</Page>
