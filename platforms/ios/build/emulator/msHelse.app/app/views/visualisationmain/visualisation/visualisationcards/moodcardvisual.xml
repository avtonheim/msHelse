<StackLayout loaded="onLoad">
<ListView style="height: 200px;" isUserInteractionEnabled="false" items="{{ SelectMoodWeekly }}">
    <ListView.itemTemplate>
        <StackLayout class="graphBox">
            <Label class="textExpl" text="{{'I den siste veka har du hatt det ' + average + ' i gjennomsnitt. 0 betyr elendig, 5 betyr middels og 10 betyr bra.'}}"  textWrap="true"/>
        </StackLayout>
    </ListView.itemTemplate>
</ListView>
</StackLayout>
