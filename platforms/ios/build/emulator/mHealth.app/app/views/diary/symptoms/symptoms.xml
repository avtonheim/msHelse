<Page loaded="onLoaded">
  <Page.actionBar>
         <Image src="images/ikon.png" class="action-image"></Image>
         <ActionBar title="mHelse" class="action-bar"></ActionBar>
         <NavigationButton text="Tilbake" tap="tapHome"/>
     </Page.actionBar>
  <ScrollView>

    <StackLayout class="content">
      <Label text="Symptom"/>

      <Label textWrap="true" text="Legg inn opplysningar om dagen din her."/>
      <TextField id="subject" hint="Legg inn symptom" />
      <TextView id="message" hint="Legg inn tekst" />
      <Button text="Send inn" tap="onTap"/>
      <Progress value="{{ someProperty }}" />
      <Button class="nxtBtn" text="Neste" tap="onNavTodo"/>
    </StackLayout>

  </ScrollView>
</Page>
