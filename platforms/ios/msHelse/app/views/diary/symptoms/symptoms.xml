<Page loaded="onLoaded">
  <Page.actionBar>
         <Image src="images/ikon.png" class="action-image"></Image>
         <ActionBar title="mHelse" class="action-bar">
           <StackLayout>
              <Image src="~/images/tittel.png" class="iconTitle" ios.position="center"></Image>
           </StackLayout>
           <ActionItem text="Neste" tap="onNavText" ios.position="right"/>
         </ActionBar>
     </Page.actionBar>

    <StackLayout>
      <Progress value="{{ someProperty }}"/>
      <Label class="content" text="Symptom" style="font-weight: bold;"/>
      <Label class="content" text="Her kan du legge inn dine symptom sidan sist du fullførte dagboka di. Du kan velje fleire symptom" textWrap="true"/>

      <GridLayout rows="*,*,*" columns="*,*" width="350" height="250">
        <Button class="symBtn" row="0" col="0" text="Fatigue" tap="{{ insert }}"/>
        <Button class="symBtn" row="0" col="1" text="Søvnproblem" tap="{{ insert }}"/>
        <Button class="symBtn" row="1" col="0" text="Husk" tap="{{ insert }}"/>
        <Button class="symBtn" row="1" col="1" text="Synsproblem" tap="{{ insert }}"/>
        <Button class="symBtn" row="2" col="0" text="Spasme" tap="{{ insert }}"/>
        <Button class="symBtn" row="2" col="1" text="Nummenhet" tap="{{ insert }}"/>
      </GridLayout>
    </StackLayout>
</Page>
