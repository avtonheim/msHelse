<Page navigatingTo="onNavigatingTo">
    <Page.actionBar>
        <ActionBar class="action-bar">
          <StackLayout>
             <Image src="~/images/tittel.png" class="iconTitle" android.position="actionBar" ios.position="center"></Image>
          </StackLayout>
            <ActionBar.actionItems>
                <ActionItem text="Ny" tap="{{ insert }}" ios.position="right" />
            </ActionBar.actionItems>
        </ActionBar>
    </Page.actionBar>
        <ListView id="listView" items="{{ lists }}" class="listLayout">
            <ListView.itemTemplate>
                <StackLayout orientation="horizontal" class="list-view-item">
                    <Label class="listItems" text="{{ list_name }}"/>
                </StackLayout>
            </ListView.itemTemplate> 
        </ListView>
</Page>
