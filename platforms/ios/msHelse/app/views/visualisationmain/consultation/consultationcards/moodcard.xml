<StackLayout loaded="onLoad">
<ListView style="height: 250px;" isUserInteractionEnabled="false" items="{{ SelectMoodMonth }}">
    <ListView.itemTemplate>
        <StackLayout class="graphBox" style="background-color: #EBA0A0">
            <Label class="textExpl" text="{{'Sidan sist konsultasjon har du hatt det ' + average + ' i gjennomsnitt. 0 betyr elendig, 5 betyr middels og 10 betyr bra. '}}"  textWrap="true"/>
        </StackLayout>
    </ListView.itemTemplate>
</ListView>
</StackLayout>
